package com.T.JavaTest.Assignment;

public class App 
{
    public static void main( String[] args )
    {
    	Player1 p1 = new Player1("abc");
        Player2 p2 = new Player2("def");
        for(int i=0; i<10; i++)
        {
        p1.sendMessage(p2.msg,i);
        System.out.println(p2.receiveMessage());
        }       
    }
}
